generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Snippet {
  id        Int      @id @default(autoincrement())
  firstName String
  lastName  String
  code      String
  language  String
  stdIn     String?
  stdOut    String
  createdAt DateTime @default(now())
}
